{
  "manifest_version": 3,
  "name": "Pass 10x Report Generator",
  "version": "3.5.0",
  "permissions": [
    "scripting",
    "activeTab",
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "https://www.pass10x.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "pass10xreportgenerator_16.jpg",
    "32": "pass10xreportgenerator_32.jpg",
    "48": "pass10xreportgenerator_48.jpg",
    "128": "pass10xreportgenerator_128.jpg"
  }
}