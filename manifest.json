{
  "manifest_version": 3,
  "name": "Pass 10x Report Generator",
  "version": "2.0",
  "permissions": [
    "scripting",
    "activeTab",
    "downloads"
  ],
  "host_permissions": [
    "https://www.pass10x.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}